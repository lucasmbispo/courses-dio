});

for (let i = 0; i < 6; i++) {

  const valorInformadoPeloUsuario = readline;

}

  if(valorInformadoPeloUsuario < 0){
    this._valorInformadoPeloUsuario = negativos.push(); 
  }
    if(valorInformadoPeloUsuario > 0){
    this._valorInformadoPeloUsuario = positivos.push(); 
